#include "urlib.h"
void mirror(struct node *root)
{
	if(root!=NULL){
		struct node *cop=root->left;
		root->left=root->right;
		root->right=cop;
		mirror(root->left);
		mirror(root->right);
	}
}
void doubletree(struct node *root)
{
	if(root!=NULL){
		struct node *copy=malloc(sizeof(struct node));
		copy->data=root->data;
		copy->left=root->left;
		root->left=copy;
		doubletree(copy->left);
		doubletree(root->right);
	}
}
		
main()
{
	struct node *root=NULL;
	int c;
	char s[5];
	for(scanf("%s",s);strcmp(s,"x");scanf("%s",s)){
		c=atoi(s);
		root=addtree(root,c);
	}
	printtree(root);
//	printpath(root);
	doubletree(root);
	printf("\n");
	printtree(root);
      printpath(root);

}	
